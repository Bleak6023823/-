local Tab = Window:MakeTab({
    Name = "玩家",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Plr = game.Players.LocalPlayer.Name
Tab:AddTextbox({
	Name = "速度",
	Default = "16",
	TextDisappear = false,
	Callback = function(Value)
	while true do
  game.workspace[Plr].Humanoid.    WalkSpeed = Value
  wait()
  end
    end	
})
